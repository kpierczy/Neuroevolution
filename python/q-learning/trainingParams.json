{
    "computeDevices": "-1",
    "env": "Breakout-ramDeterministic-v0",
    "paths": {
        "savesDir" : "python/q-learning/saves",
        "initialModelName" : false,
        "initialReplayMemoryName" : false
    },
    "time": {
        "maxFramesNum" : 1000000,
        "maxFramesPerGame" : 9000,
        "learningFrequency" : 4,
        "evaluationFreq" : 20000 ,
        "evaluationGames" : 5
    },
    "agent": {
        "replayMemorySize" : 1000000,
        "initialRandomFrames" : 40000,
        "clipReward" : true,
        "trainingFrameKeep" : 1,
        "evaluationFrameKeep" : 1,
        "stackedStateLength" : 1,
        "epsilonPolicy" : "linear",
        "epsilonPolicyConfig": {
            "linear": {
                "initial" : 1,
                "initialPeriod" : 40000,
                "firstTarget" : 0.1,
                "firstTargetPeriod" : 360000,
                "finalTarget" : 0.01,
                "finalTargetPeriod" : 400000
            },
            "exponential": {
                "initial" : 1,
                "decay" : 0.0995
            }
        }
    },
    "environment" : {
        "evaluationRandomStart" : 10,
        "evaluationRandomStartAction" : 1,
        "infoAsDone" : "lifeLost"
    },
    "model": {
        "optimizer" : "Adam",
        "lossFunction" : "mse",
        "learningRate" : 25e-5,
        "batchSize" : 32,
        "layers" : [
            {"type" : "flatten"},
            {"type" : "dense", "units" : 128, "activation" : "relu", "initializer" : "varianceScaling", "scale" : 2},
            {"type" : "dense", "units" : 128, "activation" : "relu", "initializer" : "varianceScaling", "scale" : 2}
        ],
        "name" : "DQN_Breakout_player"
    },
    "log":{
        "directory" : "python/q-learning/logs",
        "logID" : "Night_run_1",
        "display" : true,
        "displayEval" : true,
        "meanTrainingScoreLength" : 100,
        "verboseLearning" : false,
        "trainingLogUpdateFreq" : 10
    }


}